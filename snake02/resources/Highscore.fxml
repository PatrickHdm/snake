<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="hdm.org.se2.snake02.HighscoreController">
	
	
	<BorderPane fx:id="HighscoreField" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
		<top>
			<HBox id="header">
				<Label id="headline">Highscore</Label>
			</HBox>
		</top>
		
		<center>
			<VBox fx:id="pHighscore" id="body"></VBox>
		</center>
		
		
		<bottom>
			<HBox id="footer">
				<Button fx:id="backToMenu" onAction="#toMenu">Back</Button>
			</HBox>
		</bottom>
		
	</BorderPane>
	
</AnchorPane>	


<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="hdm.org.se2.snake02.Menu">
	
	<BorderPane >
		<top>
			<Label >Welcome by Snake 0 two</Label>
		</top>
		
		<center>
			<VBox>
				<Button fx:id="setGameScene" onAction="#changeScene">Play</Button>
				<Button fx:id="setHighscoreScene" onAction="#changeScene">Highscore's</Button>
				<Button fx:id="setSettingsScene" onAction="#changeScene">Settings</Button>
				<Button onAction="#closeProgram">Quit</Button>
			</VBox>
		</center>
		
		<bottom>
			<Label>HDM</Label>
		</bottom>
		
	</BorderPane>

</AnchorPane>

